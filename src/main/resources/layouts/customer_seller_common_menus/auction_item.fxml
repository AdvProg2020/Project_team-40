<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="client.view.account_menus.customer_seller_common_view.AuctionItemManager"
            prefHeight="40.0" prefWidth="744.0">

    <children>
        <HBox prefHeight="32.0" prefWidth="744.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="200.0">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="200.0">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="200.0">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <HBox fx:id="buttons" prefHeight="42.0" prefWidth="144.0">
                    <children>
                        <JFXButton fx:id="chatRoomButton" contentDisplay="CENTER" onAction="#goToChatRoom" prefHeight="42.0" prefWidth="83.0" text="">
                            <graphic>
                                <ImageView fitHeight="30.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../images/icons8-chat-room-64.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </JFXButton>
                        <JFXButton fx:id="viewAuctionButton" contentDisplay="CENTER" onAction="#viewAuctionInfo" prefHeight="48.0" prefWidth="83.0" text="">
                            <graphic>
                                <ImageView fitHeight="29.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../images/icons8-purchase-order-48.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </JFXButton>
                    </children>
                </HBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
