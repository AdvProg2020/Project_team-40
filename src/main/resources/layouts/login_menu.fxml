<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="mainInnerPane" prefHeight="610.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.register_login_view.LoginManager">
    <children>
        <TextField fx:id="usernameField" layoutX="445.0" layoutY="263.0" prefHeight="25.0" prefWidth="219.0" promptText="username">

        </TextField>
        <PasswordField fx:id="passwordField" layoutX="445.0" layoutY="315.0" prefHeight="25.0" prefWidth="219.0" promptText="password">
            
        </PasswordField>
        <Label alignment="BASELINE_CENTER" layoutX="176.0" layoutY="114.0" prefHeight="73.0" prefWidth="742.0" text="Want to go to your personal account?">
            <font>
                <Font size="30" />
            </font>
        </Label>
        <Label alignment="BASELINE_CENTER" layoutX="475.0" layoutY="463.0" prefHeight="17.0" prefWidth="151.0" text="Don't have an account?">

        </Label>
        <Label fx:id="errorMessage" alignment="BASELINE_CENTER" layoutX="207.0" layoutY="421.0" prefHeight="32.0" prefWidth="426.0">

        </Label>
        <Button fx:id="login" layoutX="480.0" layoutY="371.0" onMouseClicked="#login" prefHeight="25.0" prefWidth="141.0" text="Sign In!">

        </Button>
        <Button fx:id="register" layoutX="480.0" layoutY="508.0" onMouseClicked="#register" prefHeight="25.0" prefWidth="141.0" text="Sign Up Now!">

        </Button>
    </children>

</Pane>
